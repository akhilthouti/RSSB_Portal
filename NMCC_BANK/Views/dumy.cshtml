    <script type="text/javascript">
        $(document).scannerDetection({
            timeBeforeScanTest: 200, // wait for the next character for upto 200ms
            avgTimeByChar: 40, // it's not a barcode if a character takes longer than 100ms
            preventDefault: false,
            endChar: [13],
            onComplete: function (barcode, qty) {
                debugger;
                validScan = true;
                $('#Barcode').val(barcode);
                $('#blackbgscan').hide();
                $('#backgrounddisable').hide();
                $('html, body').css('overflow-y', 'auto');
                $("#loader_mdl3").hide();
                swal("Scan successfully Done!!!");
                var data = $("#Barcode").val();
                var msg;
                $("#loader_mdl3").html("<div style='position: fixed;z-index: 5000;left: 50%;height: 100px;width: 100px;top:41%'><img class='ajax-loader' src='../Images/LoadSpinner.gif' height='100px' width='100px' align='middle' alt='ajaxloadergif' /></div>");
                $('#backgrounddisable').show();
                $("#loader_mdl3").show();
                $('#blackbg111').show();
                if (data != "") {
                    $.ajax({
                        url: "/KYCQuickEnroll/ScaningQRCode/",
                        type: "POST",
                        processData: false,
                        contentType: "application/json; charset=utf-8",
                        data: JSON.stringify({ msg: data }),
                        success: function (result) {
                            debugger;
                            if (result == "Session Expired") {
                                window.location.href = '/AccountLogin/AccountDetails?strLoginDetail=LogIn';
                            }
                            if (result.XMLStatus == "Success") {
                                swal("QR Scan KYC Done Successfully");
                                $('#testcustomerData').show();
                                $('#backgrounddisable').hide();
                                $("#loader_mdl3").hide();
                                $('#Barcode').attr('readonly', false);
                                $('#Barcode').val('');
                                $('#LblFirstName').addClass('active');
                                $("#firstname").val(result.FirstName);
                                $('#LblMiddleName').addClass('active');
                                $("#middlename").val(result.MiddleName);
                                $('#LblLastName').addClass('active');
                                $("#lastname").val(result.LastName);
                                $('#LblDateOfBirth').addClass('active');
                                $("#dob").val(result.DateOfBirth);

                                if (result.Gender == 'F') {
                                    $("#GENDER2").prop('checked', true);
                                } else {
                                    $("#GENDER1").prop('checked', true);
                                }
                                document.getElementById("imgid").src = result.XMLImageBase64;
                            } else {
                                swal(result);
                            }
                        }
                    })
                }
                else {
                    swal("Please Scan the Document");
                    $("#loader_mdl3").hide();
                }
                //$('#blackbg111').hide();
            } // main callback function	,
        });
    </script>
