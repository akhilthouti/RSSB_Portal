                            swal('Document not saved');
                        }
                        else {
                            swal('Try Again..!')
                        }
                    }
                });

            });
            $('#btnclear').click(function () {
                debugger;
                signaturePad.clear();
                drawSignatureLine();
            });

            function drawSignatureLine() {
                var context = canvas.getContext('2d');
                context.lineWidth = .5;
                context.strokeStyle = '#333';
                context.beginPath();
                context.moveTo(0, 150);
                context.lineTo(500, 150);
                context.stroke();
            }
        });
        
    </script>
