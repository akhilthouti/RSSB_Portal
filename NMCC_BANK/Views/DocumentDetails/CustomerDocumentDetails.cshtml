            function loadImage(input) {
                debugger;
                if (input.files && input.files[0]) {
                    var reader = new FileReader();
                    canvas = null;
                    reader.onload = function (e) {
                        debugger;
                        image = new Image();
                        image.onload = validateImage;
                        image.src = e.target.result;
                    }

                    reader.readAsDataURL(input.files[0]);
