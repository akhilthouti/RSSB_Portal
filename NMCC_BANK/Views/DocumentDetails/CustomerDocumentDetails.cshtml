                $.ajax({
                    url: '/DocumentDetails/DOCImageCapture?DocType=' + 'DOCSign' + "&POItype=" + docvalue + "&IdProof=" + IdProof,
                    type: 'POST',
                    data: formData,
                    dataType: 'json',
