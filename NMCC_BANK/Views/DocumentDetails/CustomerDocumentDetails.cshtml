                    }
                    function applyCrop() {
                        debugger;

                        var imageObj = $("#viewport")[0];
                        // $("#views").empty();
                        $("#ViewNew").empty();
                        $("#ViewNew").show();
                        $("#ViewNew").append("<canvas id=\"canvas123\">");
