                    processData: false,
                    success: function (Result) {
                        debugger;
                        if (Result == 'Success' ) {
                            debugger;
                            $('#div_DocSign_camera').hide();
                            $('#CameraCustDA').hide();
                            // $('#CorrImgExist').val('ImgExist');
                            $('#CameraOpenViewMainAddrs').append($('#CameraOpenViewAddrsSign'));
                            swal('Customer Signature Captured');
                            var DocMainType = "SI";
                            $.ajax({
                                url: '/DocumentDetails/GetDocumentsForGridView?DocMainType=' + DocMainType,
