                    //fileData = input.files[0];
                    fileData.append(input.files[0].name, input.files[0]);


                    $.ajax({
                        url: "/DocumentDetails/ToSavePdf",
                        type: "POST",
                        contentType: false, // Not to set any content header
                        processData: false,
                        data: fileData,
                        success: function (result) {
                            debugger;
                            $('#viewport1').hide();
                            imagePath = result;
                            $("#cropbutton").show();
