    <script>
        function saveofDecumentValidation() {

            $.ajax({
                url: '/DocumentDetails/GetDocumentsForValidation',
                type: "GET",
                contentType: false, // Not to set any content header
                processData: false, // Not to process data
                data: {},
                async: false,
                success: function (result) {

                    if (result[0] == false) {
                        swal("Please Upload Identity Document");
                    }
                    else if (result[1] == false) {
                        swal("Please Upload Address Document");
                    }
                    else if (result[2] == false) {
                        swal("Please Upload Signature Document");
                    }
                    else {
                        
                        window.location.href = "/NEWIPVDetails/CustIPVdetails";
                    }

                },
                error: function (err) {
                    debugger;
                    //  alert(err.statusText);
                }
            });

        }
    </script>
