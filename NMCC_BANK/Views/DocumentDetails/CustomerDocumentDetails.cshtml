                                    debugger;
                                    $('#docdetails_Sign').html(result);
                                    $("#l2Digi").hide();
                                    $('#overlay_disabl').hide();
                                },
                                error: function (err) {
                                    debugger;
                                    alert(err.statusText);

                                }
                            });

                            

                        }   
                         
                        else if (Result == 'Document not saved') {
                            $('#POAC_camera').hide();
                            $('#CameraCustDA').hide();
                            $('#ImageUpload').val('');
                            $('#CorrViewDoc').show();
                            $('#CorrImgExist').val('ImgExist');
