                            image.src = canvas.toDataURL("image/png");
                        } else restartJcrop();
                    }

                    function restartJcrop() {
                        debugger;
                        if (jcrop_api != null) {
                            jcrop_api.destroy();
                        }
                        $("#views").show();
                        $("#views").empty();


                        $("#views").append("<canvas id=\"canvasPOI\">");
