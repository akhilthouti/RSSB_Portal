                            PDFJS.workerSrc = '../../Scripts/pdf.worker.js';
                            var url = result;
                            var pdfDoc = null,
                                pageNum = 1,
                                pageRendering = false,
                                pageNumPending = null,
                                //rotateangle=0,
                                scale = 1;
                            $('#viewcanvas').append('<div id="viewcanvas1" style="border: 2px solid #609fef;border-radius:7px;padding: 6px 14px 17px 16px;margin-right: 11px;margin-left: 11px;"></div>');
                            $("#viewcanvas1").append('<canvas id="viewport" class="loaderckycimg" width="842" height="1190" style="border: 1px solid black;margin-left: 3% !important ;padding-top:6px;left: 37px;background-color:#fff"></canvas>');
