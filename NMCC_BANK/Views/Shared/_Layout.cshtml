    <style>
        .init_div {
            display: flex;
            justify-content: space-between;
            align-items: center;
            height: 90px;
        }

        .init_logo_div {
            margin-left: 40px;
        }

        .init_logo_layout {
            height: 53px;
        }

        .init_ul {
            display: flex;
            flex-direction: row;
            margin-bottom: 0;
            align-items: center;
        }

        .init_li {
            padding: 2px;
            list-style-type: none;
        }

        .init_a {
            font-size: 15px;
            font-weight: 600;
            margin-right: 12px;
            padding: 6px 8px;
            color: #fff;
        }

            .init_a:hover {
                color: #fff;
                border-bottom: 1px solid #fff !important;
                transition: ease-in-out 0.3s;
            }

            .init_a:focus {
                border: none !important;
            }

        .init-ul-parent {
            margin-right: 40px;
        }

        .init_logout_img {
            cursor: pointer;
            width: 33px;
            height: 33px;
        }

        @@media screen and (max-width:768px) {
            .init_logo_div {
                margin-left: 15px;
            }

            .init-ul-parent {
                margin-right: 15px;
            }

            .init_logo_layout {
                height: 44px;
                width: 160px;
            }

            .init_a {
                margin-right: 0px;
            }
        }
    </style>
