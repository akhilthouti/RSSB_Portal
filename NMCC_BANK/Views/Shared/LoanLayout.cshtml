    <style>
        .init_disable_li {
            pointer-events: none;
        }

        .lgouticon {
            width: 33px;
            height: 33px;
            margin: 0%;
        }

        .logonew {
            width: 240px;
            height: 61px;
            margin-top: 6px;
            margin-left: 8%;
        }



        .bg-green {
            background-color: green !important;
        }


        #tab1 {
            width: 100% !important;
            float: left !important;
        }

        .progressbar {
            counter-reset: step;
        }

            .progressbar li {
                position: relative;
                list-style: none;
                float: left;
                color: #676665;
                float: left;
                /* width: 12.2%;*/
                /* margin-left: 0.7%;*/
                margin-left: 4.7%;
                text-align: center;
            }
                /*.progressbar li:hover {
                                        background-color:#f76217;
                                        color:#0f7041;
                                    }*/
                /* Circles */
                .progressbar li:before {
                    content: counter(step);
                    counter-increment: step;
                    width: 42px;
                    height: 40px;
                    border-top: none;
                    display: block;
                    border-radius: 50%;
                    margin: -15px auto -9px auto;
                    /* border-right: 83px; */
                    background-color: #fff;
                    line-height: 36px;
                    MARGIN-BOTTOM: -3PX !important;
                    font-weight: 600;
                }


                .progressbar li:after {
                    content: "";
                    position: absolute;
                    width: 100%;
                    height: 2px;
                    /*background: #5186e5;*/
                    top: 20px; /*half of height Parent (li) */
                    left: -50%;
                    z-index: -1;
                }

                .progressbar li:first-child:after {
                    content: none;
                }

                .progressbar li.active:before {
                    /*background: #238a58;*/
                    background: #0f7041;
                    content: "✔";
                    color: #fff;
                }

        .agent_tab {
            FONT-SIZE: 19PX;
            FONT-WEIGHT: 700;
        }
        /*.progressbar li.active + li:after {
                                            background: #12653d;
                                            border:1px solid #12653d;
                                        }*/

        #header {
            top: 0 !important;
        }

        .nav-tabs > li {
            z-index: 1000;
        }

        #topbar {
            width: 100%;
        }

        @@media screen and (min-device-width:300px) and (max-device-width:768px) {
            .lgouticon {
                width: 25px;
                height: 25px;
                margin: 0%;
            }

            .logonew {
                width: 186px !important;
                height: 51px !important;
                margin-top: 8px !important;
                margin-left: 4% !important;
            }

            .agent_tab {
                font-size: 12px !important;
            }

            .prgrsbrul {
                width: 100%;
            }

            .prgrslst {
                width: 18%;
                margin-left: 0% !important;
            }

            .list-a {
                font-size: 12px !important;
            }

            .progressbar li:before {
                content: counter(step);
                counter-increment: step;
                width: 42px;
                height: 40px;
                border: 1px solid #852e2e;
                border-top: none;
                display: block;
                color: #f71717;
                border-radius: 50%;
                box-shadow: -1px 2px 6px 0px rgb(129 48 48);
                margin: -15px auto -9px auto;
                /* border-right: 83px; */
                background-color: #fff;
                line-height: 36px;
                MARGIN-BOTTOM: -3PX !important;
                font-size: 14px;
            }
        }

        @@media screen and (min-device-width:768px) and (max-device-width:1003px) {
            .agent_tab {
