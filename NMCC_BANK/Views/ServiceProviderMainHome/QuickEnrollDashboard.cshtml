        .progressbar {
            counter-reset: step;
        }

            .progressbar li {
                position: relative;
                list-style: none;
                float: left;
                color: #676665;
                float: left;
                width: 12.2%;
                margin-left: 0.7%;
                text-align: center;
            }
                /*.progressbar li:hover {
                background-color:#f76217;
                color:#0f7041;
            }*/
                /* Circles */
                .progressbar li:before {
                    content: counter(step);
                    counter-increment: step;
                    width: 42px;
                    height: 40px;
                    border: 1px solid #5186e5;
                    border-top: none;
                    display: block;
                    color: #f76217;
                    border-radius: 50%;
                    box-shadow: -1px 2px 6px 0px rgb(132, 173, 245);
                    margin: -15px auto -9px auto;
                    /* border-right: 83px; */
                    background-color: #fff;
                    line-height: 36px;
                }


                .progressbar li:after {
                    content: "";
                    position: absolute;
                    width: 100%;
                    height: 2px;
                    /*background: #5186e5;*/
                    top: 20px; /*half of height Parent (li) */
                    left: -50%;
                    z-index: -1;
                }

                .progressbar li:first-child:after {
                    content: none;
                }

                .progressbar li.active:before {
                    /*background: #238a58;*/
                    background: #0f7041;
                    content: "✔";
                    color: #fff;
                }

        /*.progressbar li.active + li:after {
                    background: #12653d;
                    border:1px solid #12653d;
                }*/
