    DEDUPE_GRID_MAIN1 obj = new DEDUPE_GRID_MAIN1();
    list = Model.dedupegridlistss;//Model.dedupegridlistss;
}
@{
    Layout = null;
}

<script type="text/javascript">
    function dedupe_AccNo() {
        debugger;
       $("#init-btn").hide();
       $("#myModal").hide();
        
        var radiobutton1 = $("input[name='dedupe']:checked").attr('id');
        var AccountNo = document.getElementById("test001").innerText;

        $.ajax({
            //url: '/KYCQuickEnroll/GetCBSDatadedupe',
            url: '/KYCQuickEnroll/GetCBSData1',
            type: 'POST',
            data: {
                CBSValue1: radiobutton1

            },
            cache: false,
            success: function (data) {
                 debugger;
                  $('#ExistCustomerDiv').show();
               
                $("#ExistFirstName").val(data.existFirstName);
                 
                               

                               
                                $("#ExistMiddleName").val(data.existMiddleName);
                               

                             
                                $("#ExistLastName").val(data.existLastName);
                                $("#ExistMobileNo").val(data.existMobileNo);
                                $("#ExistEmail").val(data.existEmail);
                                $("#ExistGender").val(data.existGender);
                                $("#ExistLastName").val(data.existLastName);
                                $("#ExistAddress1").val(data.existAddress1);
                                $("#ExistAddress2").val(data.existAddress2);
                                $("#ExistAddress3").val(data.existAddress3);
                                $("#ExistCity").val(data.existCity);
                                $("#ExistAnnalIncome").val(data.existAnnalIncome);
                                $("#ExistOccupation").val(data.existOccupation);
                                $("#ExistPincode").val(data.existPincode);
                                $("#ExistDOB").val(data.existDOB);
            
               
                
            }
        });
    }

    
</script>
