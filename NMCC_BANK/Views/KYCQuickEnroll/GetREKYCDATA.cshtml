        function GetOTP() {
            debugger;
            var EmailId = $('#emailcodeOTP').val();
            $.ajax({
                url: '/KYCQuickEnroll/EmailOTPSent',
                type: 'POST',
                data: {
                    EmailId: EmailId
                },
                cache: false,
                success: function (result) {
                    debugger;
                    if (result != null) {
                        swal(result);
                    } else {
                        swal('OTP does not send.try again..');
                    }
                    // $('#DigitCaptureImage').html(result);
                }
            });

        }
        function OTPAuthenticate() {
            debugger;
            var OTP = $('#emailauth').val();
            if (OTP == '') {
                swal("Enter valid OTP...");
                return false;
            }
            $.ajax({
                url: '/KYCQuickEnroll/OTPAutheticate',
                type: 'GET',
                data: {
                    OTP: OTP
                },
                cache: false,
                success: function (result) {
                    debugger;
                    if (result == "Athenticate Successfully... ") {
                        swal(result);
                        $('#Emailbaknew1').hide();
                        $('#emailformOTP').hide();
                        $('#EmailGetOtp').hide();
                        $('#isEmailVerify').show();


                    } else {
                        swal(result);
                        $('#Emailbaknew1').hide();
                        $('#emailauth').val('');

                    }
                    // $('#DigitCaptureImage').html(result);
                }
            });

        }
