        function IsEmail(email) {
            var regex = /^[_a-z0-9-]+(\.[_a-z0-9-]+)*@@[a-z0-9-]+(\.[a-z0-9-]+)*(\.[a-z]{2,4})$/;
            if (!regex.test(email)) {
                return false;
            } else {
                return true;
            }
        }
        

