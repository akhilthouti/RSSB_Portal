    <script type="text/javascript">
        function aadharChek(e) {
            var Adhatest = $('#aadharno').val();
            var account = document.getElementById("aadharno");
            var aadhaarnumber = /^[0-9]*$/;
            if (!account.value.match(aadhaarnumber)) {
                swal('Please Enter Only Numbers. ');
                $('#aadharno').val('');
                return false;
            }
            if (Adhatest != "") {
                if (Adhatest.length != 12) {
                    swal("Please Enter 12 Digit");
                    $('#aadharno').val('');
                    return true;
                }
                chekCustO();
            }
        }
        


        
        

        function chekCustO() {
            var url = '/DataVerify/aadharChek?aadhar=' + $('#aadharno').val();
            $.ajax({
                url: url,
                type: "GET",
                success: function (result) {
                    if (result != 'valid') {
                        swal(result);
                        $('#aadharno').val('');
                    }
                }
            })
        }
    </script>
    
      
    <script type="text/javascript">
        $(document).scannerDetection({
