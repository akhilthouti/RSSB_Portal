            var selectedValue = "";
            $("#VTYPE").on("change", function () {
                selectedValue = $(this).val();
                var input = $("#QEVTypeTextboxId");
                var placeholderText = "";
                var pattern = "";

                if (selectedValue === "P1") {
                    placeholderText = "Enter PAN No";
                    pattern = "^[A-Z]{5}[0-9]{4}[A-Z]{1}$";
                    input.attr("maxlength", 10);
                    input.on("input", function () {
                        $(this).val($(this).val().toUpperCase());
                    });
                } else if (selectedValue === "A") {
                    placeholderText = "Enter Aadhaar No";
                    pattern = "^[2-9]{1}[0-9]{11}$";
                    input.attr("maxlength", 12);
                    input.off("input");
                } else {
                    input.hide();
                    return;
                }

                input.attr("placeholder", placeholderText).attr("pattern", pattern).show();
            });

            $("#QEVTypeTextboxId").on("blur", function () {
                debugger;
                var inputValue = $(this).val();
                var pattern = $(this).attr("pattern");

                if (inputValue && !inputValue.match(pattern)) {
                    if(selectedValue == "P1")
                    {
                        swal("Invalid Pan Number", "Please Enter a Valid Pan Number.", "warning");
                        $(this).val("").focus();
                    }
                    else if(selectedValue == "A")
                    {
                        swal("Invalid Aadhar Number", "Please Enter a Valid Aadhar Number.", "warning");
                        $(this).val("").focus();
                    }
                    
                }
                
            });

        });
