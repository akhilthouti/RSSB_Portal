    <style>
        #show {
            display: none;
        }

        .init_div {
            display: flex;
            justify-content: space-between;
            align-items: center;
            height: 90px;
        }

        .init_a {
            font-size: 18px;
            font-weight: 500;
        }

            .init_a:focus {
                border: none !important;
            }

        .init_login_btn {
            border: none !important;
            color: #fff !important;
            font-size: 18px !important;
            color: #fff !important;
            margin-top: 0 !important;
            border-radius: 0px !important;
            padding: 5px 15px !important;
            font-weight: 500 !important;
        }

        .init_link {
            font-size: 18px;
            margin-right: 12px;
            padding: 3px 10px;
            color: #007bff !important;
        }

            .init_link:hover {
                text-decoration: underline;
                transition: all.6s ease;
            }

        .init_ul {
            display: flex;
            flex-direction: row;
            margin-bottom: 0;
        }

        .init-ul-parent {
            margin-right: 40px !important;
        }

        .init_nav {
            width: 100%;
            font-family: 'Roboto', sans-serif;
            background: white;
            min-height: 95px;
            margin-bottom: 0 !important;
            padding: 0;
        }

        .init_logo_div {
            margin-left: 40px;
        }
        .navbar-expand-lg .navbar-nav .dropdown-menu {
            position: absolute;
            background-color: #bf5252;
            color: #fff;
        }

        .dropdown-menu > li > a:focus, .dropdown-menu > li > a:hover {
            color: #262626;
            text-decoration: none;
            background-color: #ba0000;
            background-image: linear-gradient(359deg, black, transparent);
        }

        .init_li {
            border-bottom: 1px solid rgba(255,255,255,0.5);
            padding: 2px;
        }

        .dropdown-menu_custom {
            width: 128px !important;
            padding: 0 !important;
            border-top: none;
            border-radius: 0px;
            width: 130px !important;
            position: absolute;
            z-index: 1000;
            color: #212529;
            text-align: left;
            list-style: none;
            background-clip: padding-box;
            border: 1px solid rgba(0, 0, 0, 0.15);
            border-radius: 0.25rem;
        }

        #CustomerClickId:focus {
            outline-color: none;
            outline-offset: 0px;
        }

        #AgentClickId :focus {
            outline-color: none;
            outline-offset: 0px;
        }

        .dropdown-menu > li > a:hover {
            background: transparent !important;
        }

        .dropdown-menu_custom > li:focus, .dropdown-menu_custom > li:hover {
            border-bottom: 2px solid #fff;
            transition: all 0.3s ease;
        }

        .img-res {
            height: 41px;
            width: 212px;
            margin-left: -25px;
        }

        button:focus {
            outline: none !important;
        }

        /*main home page*/
        @@media only screen and (max-width:800px) {
            .init_logo_main {
                height: 44px;
            }
        }

        @@media only screen and (max-width:700px) {
            .init_logo_main {
                height: 40px;
            }
        }

        @@media only screen and (max-width:700px) {
            .init_logo_div {
                margin-left: 20px;
            }
        }

        @@media only screen and (max-width:700px) {
            .init_link {
                display: none;
            }
        }

        @@media only screen and (max-width:700px) {
            .init_logo_main {
                height: 34px;
