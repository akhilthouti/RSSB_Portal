    <script>
        function SubmitofServices() {

            var panNumber = $("#valueofidtypeforPANID").val().trim().toUpperCase();
            if (panNumber === "") {
                swal("Please Enter PAN Number");
                return false;
            }

            // Regular expression pattern for PAN validation
            var panPattern = /^([A-Z]){5}([0-9]){4}([A-Z]){1}?$/;

            if (!panPattern.test(panNumber)) {
                swal("Invalid PAN Number", "Please enter a valid PAN Number", "error");
                return false;
            }

            //if ($("#valueofidtypeforPANID").val() == "") {
            //    swal("Please Enter PanNumber");
            //    return false;
            //}

            var panPattern = /^([a-zA-Z]{5})(\d{4})([a-zA-Z]{1})$/;
            if (!panPattern.test(panNumber)) {
                swal("Invalid PAN Number", "Please enter a valid PAN Number", "error");
                return false;
            }

            var checkboxes = document.querySelectorAll(".check_boxservice");
            var isChecked = false;

            checkboxes.forEach(function (checkbox) {
                if (checkbox.checked) {
                    isChecked = true;
                }
            });

            if (!isChecked) {
                // Display an error message using SweetAlert
                swal("Error", "Please select at least one checkbox.", "error");
                return false;
            }


            var companyforCIN = $('#CompanyNameID').val();
            var companyforMSME = $('#CompanyNamesID').val();
            if (companyforCIN == companyforMSME) {
                window.location.href = "/CurrentCAF/CurrentCAF";
            }
            else {

                swal({
                    title: "Fetched Company Name Is Not Matching",

                    type: 'warning',

                    confirmButtonColor: '#DD6B55',
                    confirmButtonText: 'Ok',

                    closeOnConfirm: false,
                    closeOnCancel: false
                },
                    function (isConfirm) {

                        if (isConfirm) {
                            window.location.href = "/CurrentCAF/CurrentCAF";

                        } else {
                            swal("Cancelled", "please Enter Correct Company Name");
                        }
                    });
            }

        }
    </script>
