                $.ajax({
                    url: '/CurrentServices/IDVerifyforPan?Idvalue=' + pancard + "&IdType=PanCard",
                    type: 'POST',
                    cache: false,
                    dataType: "json",
                    contentType: 'application/json; charset=utf-8',
                    success: function (result) {
                        debugger;
                        $("#init_loader").hide();
                        if (result == "Failed") {
                            swal("No Responce From API,Please try after Sometime");

                        }
                        if (result != "") {
                            $("#PANDIVID").show();

                            swal("PanCard Verified Successfully");
                            $("#CONSTITUTIONOFBUSINESSID").val(result.constitutionofbusiness);
                            $("#GSTINID").val(result.gstin);
                            $("#LEGALNAMEOFBUSINESSID").val(result.legalnameofbusiness);
