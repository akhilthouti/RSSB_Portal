    </div>
    <script>
        $(document).ready(function () {
            $("#init_loader").hide();
            $("#init_click_header1").click(function () {
                $("#inner_wrapper1").slideToggle(200);
            })
            $("#other_services").click(function () {
                $("#other_service").slideToggle(200);
            })
            $("#init_click_header2").click(function () {
                $("#inner_wrapper2").slideToggle(200);
                $("#inner_wrapper3").hide(200);
                $("#inner_wrapper4").hide(200);
                $('#init_click_header3').prop('checked', false);
                $('#init_click_header4').prop('checked', false);
            })
            $("#init_click_header3").click(function () {
                $("#inner_wrapper2").hide(200);
                $("#inner_wrapper3").slideToggle(200);
                $("#inner_wrapper4").hide(200);
                $('#init_click_header2').prop('checked', false);
                $('#init_click_header4').prop('checked', false);
            })
            $("#init_click_header4").click(function () {
                $("#inner_wrapper2").hide(200);
                $("#inner_wrapper3").hide(200);
                $("#inner_wrapper4").slideToggle(200);
                $('#init_click_header3').prop('checked', false);
                $('#init_click_header2').prop('checked', false);
            })
        });
    </script>
    <script>
        let dataForpan = "@ViewBag.dataforPan";
        if (dataForpan == "true") {
            $("#PANDIVID").show();
        }
        let dataForCIN = "@ViewBag.dataforCIN";
        if (dataForCIN == "true") {
            $("#CINDIVID").show();
        }
        let dataForGSTIN = "@ViewBag.dataforGSTIN";
        if (dataForGSTIN == "true") {
            $("#GSTINDIVID").show();
        }
        let dataForMSME = "@ViewBag.dataforMSME";
        if (dataForMSME == "true") {
            $("#MSMEDIVID").show();
        }

    </script>

