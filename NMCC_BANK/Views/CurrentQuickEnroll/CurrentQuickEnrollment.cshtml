    <script>
        function isNumeric(evt) {
            debugger;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode != 46 && charCode > 31 && (charCode < 48 || charCode > 57)) {
                swal("PLEASE ENTER NUMERIC VALUES");
                return false;
            }
            if (evt.target.value.length >= 10) {
                return false;
            }
            return true;
        }
        $(document).ready(function () {
            debugger;
            $("#CIDTYPEID").on("change", function () {
                var selectedValue = $(this).val();
                var input = $("#CTypeTextboxID");
                var placeholderText = "";
                var pattern = "";

                if (selectedValue === "Pan No") {
                    placeholderText = "Enter PAN No";
                    pattern = "^[A-Z]{5}[0-9]{4}[A-Z]{1}$";
                    input.attr("maxlength", 10);
                    input.on("input", function () {
                        $(this).val($(this).val().toUpperCase());
                    });
                } else if (selectedValue === "Aadhar") {
                    placeholderText = "Enter Aadhaar No";
                    pattern = "^[2-9]{1}[0-9]{11}$";
                    input.attr("maxlength", 12);
                    input.off("input");
                } else {
                    input.hide();
                    return;
                }

                input.attr("placeholder", placeholderText).attr("pattern", pattern).show();
            });

            $("#CTypeTextboxID").on("blur", function () {
                debugger;
                var inputValue = $(this).val();
                var pattern = $(this).attr("pattern");

                if (inputValue && !inputValue.match(pattern)) {
                    swal("Invalid Number", "Please enter a valid number.", "warning");
                    $(this).val("").focus();
                }
            });

        });

    </script>

