    <script>
        function SubmitOnQuick() {
            var aadharVerificationChecked = document.getElementById('checkforAadharVerificationID').checked;
            var digilockerChecked = document.getElementById('checkforDigilockerID').checked;
            if (!aadharVerificationChecked && !digilockerChecked) {
                swal('Error', 'Please select at least one ID verification option.', 'error');
            }
            else {
                var formData = new FormData();
                formData.append("FirstName", $("#CFirstNameID").val());
                formData.append("LastName", $("#CLastNameID").val());
                formData.append("MobileNO", $("#CMobileNoID").val());
                formData.append("EmailID", $("#CEmailID").val());
                formData.append("VerificationType", $("#CIDTYPEID").val());
                formData.append("VerificationTypeValue", $("#CTypeTextboxID").val());
                formData.append("Category", $("#CATTYPEID").val());
                $.ajax({
                    url: '/CurrentQuickEnroll/CurrentQuickEnrollment',
                    type: 'POST',
                    data: formData,
                    dataType: 'json',
                    contentType: false,
                    processData: false,
                    success: function (result) {
                        if (result == "Success") {
                            swal("Data Saved Successfully");
                            //window.location.href = '/CurrentCAF/CurrentCAF';
                            window.location.href = "/CurrentindividualDocumentDetails/CurrentIndividualDocument";
                        }
                        else {
                            swal("please try Again");
                        }

                    }
                });
            }
        }
    </script>



